{
  "name": "leetcode-practice",
  "version": "1.0.0",
  "description": "LeetCode practice repository with Python and React/TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only",
    "lint": "eslint solutions/react-ts/**/*.{ts,tsx}",
    "format": "prettier --write solutions/react-ts/**/*.{ts,tsx}"
  },
  "keywords": ["leetcode", "practice", "algorithms", "data-structures"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.198"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/solutions/react-ts/**/*.test.ts"],
    "collectCoverageFrom": [
      "solutions/react-ts/**/*.{ts,tsx}",
      "!solutions/react-ts/**/*.test.{ts,tsx}",
      "!solutions/react-ts/**/*.d.ts"
    ]
  }
}